/* 
  Name:			Fade Out Right
  Type:			Exit
  Description:	Fade Out Effect
  Version:		2.0	
  locale:		{"zh-Hans":"右侧淡出", "en-US":"Fade Out Right"}
 */
@keyframes fadeOutRight {
  0% {
    opacity: 1;
  }

  100% {
    opacity: 0;
    transform: translate3d(100%, 0, 0);
  }
}

.fadeOutRight {
  animation-name: fadeOutRight;
}